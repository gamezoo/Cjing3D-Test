{
    "configWin32": {
        "clean": [
            "build/win32",
            "bin/win32"
        ],
        "libs": [
            "cd ..\\3rdparty && .\\build_libs.bat "
        ],
        "premake": [
            "--renderer=dx11",
            "--platform_dir=win32"
        ],
        "copy": [
            [
                "assets",
                "bin/win32/x64/Debug/data/"
            ],
            [
                "assets",
                "bin/win32/x64/Release/data/"
            ],
            [
                "../3rdparty/shared_libs/win32",
                "bin/win32/x64/Debug/"
            ],
            [
                "../3rdparty/shared_libs/win32",
                "bin/win32/x64/Release/"
            ]
        ],
        "build": {
            "dir": "build/win32"
        },
        "data_dir": {
            "Debug": "bin/win32/x64/Debug/data/",
            "Release": "bin/win32/x64/Debug/data/"
        },
        "tools": {
            "jsc": "../tools/build_scripts/jsc/jsc.py",
            "pre_parser": "../tools/pre_parser/pre_parser.py",
            "premake_path": "../tools/premake/premake5"
        }
    },
    "base": {
        "tools": {
            "jsc": "../tools/build_scripts/jsc/jsc.py",
            "pre_parser": "../tools/pre_parser/pre_parser.py",
            "premake_path": "../tools/premake/premake5"
        }
    }
}